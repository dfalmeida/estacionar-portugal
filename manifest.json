{
  "manifest_version": 2,
  "name": "Estacionar Portugal",
  "description": "Gerir estacionamentos em Portugal",
  "version": "0.0.4",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "https://eos.empark.com/*",
    "storage",
    "notifications",
    "alarms"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["api.js", "common.js", "background.js"]
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_title": "Estacionar Portugal"
  }
}
